{
  "rewrites": [
    {
      "source": "/subscribers/(.*)",
      "destination": "/client/src/pages/SubscriberPage.jsx"
    },
    {
      "source": "/subscribers/names",
      "destination": "/client/src/pages/SubNamesPage.jsx"
    },
    {
      "source": "/subscribers/:id",
      "destination": "/client/src/pages/SingleSubscriberPage.jsx"
    },
    { "source": "/(.*)", "destination": "/client/src/pages/HomePage.jsx" }
  ]
}
